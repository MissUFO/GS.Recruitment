<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Gresham Recruitment Cloud Service - @ViewBag.Title</title>
    <meta name="description" content="">
    <meta name="robots" content="all,follow">

    <!-- Bootstrap Core CSS -->
    <link href="~/Content/bootstrap.min.css" rel="stylesheet">
    <!-- MetisMenu CSS -->
    <link href="~/Content/metisMenu.min.css" rel="stylesheet">
    <!-- Custom CSS -->
    <link rel="stylesheet" href="~/Content/site.controlpanel.css" id="stylesheet">
    <!-- Custom Fonts -->
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" type="text/css">

    <link rel="shortcut icon" href="/favicon.ico">
</head>
<body>
    <div id="wrapper">
       
        <nav class="navbar navbar-default navbar-static-top" role="navigation" style="margin-bottom: 0">
            <!-- header-->
            @Html.Partial("_HeaderControlPanel")
            <!-- /.header-->
            <!-- navbar top-->
            @Html.Partial("_NavBarTopControlPanel")
            <!-- /.navbar top-->
            <!-- navbar right-->
            @Html.Partial("_NavBarLeftControlPanel")
            <!-- /.navbar right-->
        </nav>
            <div id="page-wrapper">
                <!-- body-->
                @RenderBody()
                <!-- /.body-->
            </div>
            <!-- footer-->
            @Html.Partial("_FooterControlPanel")
            <!-- /.footer-->
</div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")

    @Scripts.Render("~/bundles/menu")
    @*<script type="text/javascript" src="~/Scripts/moment.min.js"></script>
    <script type="text/javascript" src="~/Scripts/bootstrap-datetimepicker.min.js"></script>*@

    <!-- Metis Menu Plugin JavaScript 
    <script src="~/Scripts/metisMenu.min.js"></script>-->
   
    <!-- Custom Theme JavaScript -->
    <script src="~/Scripts/controlpanel.js"></script>

    @RenderSection("scripts", required: false)
</body>
</html>
